                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SET NOCOUNT ON;
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
SELECT myBackupSet.backup_set_id AS BackupSetId,
       myBackupSet.name AS BackupName,
       myBackupSet.type AS BackupType
FROM dbo.restorehistory AS myRestoreHistory
INNER JOIN dbo.backupset AS myBackupSet ON myBackupSet.backup_set_id = myRestoreHistory.backup_set_id
WHERE myRestoreHistory.destination_database_name IN (SELECT myDatabases.name FROM sys.databases AS myDatabases WHERE state = 1 OR is_in_standby = 1)
	AND EXISTS (SELECT 1 FROM sys.master_files AS myMasterFiles WHERE file_id = 1 AND database_id = DB_ID (myBackupSet.database_name) AND COALESCE (myMasterFiles.redo_start_lsn, myMasterFiles.differential_base_lsn) = myBackupSet.last_lsn);</property>
